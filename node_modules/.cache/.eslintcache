!allowAlphabetsRegex.test(firstName)){\n        errors.push(\"Invalid First Name\");\n    }\n\n    if (!allowAlphabetsRegex.test(lastName)){\n        errors.push(\"Invalid Last Name\");\n    }\n\n    if (!emailRegex.test(email)){\n        errors.push(\"Invalid email\");\n    }\n\n    if (!passwordRegex.test(password)){\n        errors.push(\"Invalid password\");\n    }\n\n    if (errors.length === 0){\n      setUserValidated(true);\n    }else {\n      setErrorMessage(errors);\n    }\n  }\n\n  return (\n    !isUserValidated ? (\n        <div className=\"App\">\n        <input type=\"text\" \n            placeholder=\"First Name\" \n            style={{marginTop: \"24px\"}}\n            onChange={(event) => setFirstName(event.target.value)}/>\n\n        <input type=\"text\" \n            placeholder=\"Last Name\" \n            style={{marginTop: \"24px\"}}\n            onChange={(event) => setLastName(event.target.value)}/>\n\n        <input type=\"text\" \n            placeholder=\"email\" \n            style={{marginTop: \"24px\"}}\n            onChange={(event) => setEmail(event.target.value)}/>\n\n        <input type=\"password\" \n            placeholder=\"password\" \n            style={{marginTop: \"24px\"}}\n            onChange={(event) => setPassword(event.target.value)}/>\n\n        <label for=\"season\">Favorite Season</label>\n    \n        <select onClick={(event) => setSeason(event.target.value)}>\n            <option value=\"Spring\">Spring</option>\n            <option value=\"Summer\">Summer</option>\n            <option value=\"Fall\">Fall</option>\n            <option value=\"Winter\">Winter</option>\n        </select>\n\n        <button onClick={() => handleSubmit(firstNameInputValue, lastNameInputValue, emailInputValue, passwordInputValue, seasonInputValue)}>Submit</button>\n            <div>\n                {\n                errorMessage.map(errorMessage => (\n                    <p>{errorMessage}</p>\n                ))\n                }\n            </div>\n        </div>\n     ) : (\n        <Profile firstName = {firstNameInputValue} lastName = {lastNameInputValue} email = {emailInputValue} password = {passwordInputValue} season = {seasonInputValue}/>\n     )\n    \n  );\n}\n\nexport default Form;\n","/Users/nancy/Desktop/3172/Labs/l7_chen_siyuan/l7-chen-siyuan/src/components/Dashboard/Dashboard.js",[],[],"/Users/nancy/Desktop/3172/Labs/l7_chen_siyuan/l7-chen-siyuan/src/components/Profile/Profile.js",["42","43"],[],"import React, {useEffect, useState} from 'react';\nimport { Link } from \"react-router-dom\";\n\nfunction Profile(props) {\n  return (\n    <React.Fragment>\n        <div>{props.firstName}</div>\n        <div>{props.lastName}</div>\n        <div>{props.email}</div>\n        <div>{props.password}</div>\n        <div>{props.season}</div>\n\n        <Link to=\"/dashboard\">Go to dashboard page</Link>\n    </React.Fragment>\n  \n  )\n}\n\nexport default Profile;\n",{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":1,"column":8,"nodeType":"50","messageId":"51","endLine":1,"endColumn":12},{"ruleId":"48","severity":1,"message":"52","line":3,"column":16,"nodeType":"50","messageId":"51","endLine":3,"endColumn":25},{"ruleId":"48","severity":1,"message":"53","line":3,"column":27,"nodeType":"50","messageId":"51","endLine":3,"endColumn":35},{"ruleId":"48","severity":1,"message":"52","line":1,"column":16,"nodeType":"50","messageId":"51","endLine":1,"endColumn":25},{"ruleId":"48","severity":1,"message":"52","line":1,"column":16,"nodeType":"50","messageId":"51","endLine":1,"endColumn":25},{"ruleId":"48","severity":1,"message":"53","line":1,"column":27,"nodeType":"50","messageId":"51","endLine":1,"endColumn":35},"no-native-reassign",["54"],"no-negated-in-lhs",["55"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never used.","'useState' is defined but never used.","no-global-assig